{
	"compilerOptions": {
		"baseUrl": ".",
		"lib": ["es5", "es6"],
		"target": "ES2018",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "./output",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": false,
		"declaration": true,
		"paths": {
			"catsvsdogs": ["../@types/catsvsdogs"]
		}
	}
}
