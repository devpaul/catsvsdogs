{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"baseUrl": ".",
		"target": "ES2018",
		"module": "commonjs",
		"outDir": "./output/src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"paths": {
			"catsvsdogs": ["../@types/catsvsdogs"],
			"@catsvsdogs/server/*": ["../server/output/src/*"]
		}
	}
}
