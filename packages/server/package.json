{
  "name": "@catsvsdogs/server",
  "version": "1.0.0",
  "description": "Cats vs Dogs server",
  "main": "output/src/main.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "shx rm -rf output",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node --project ./tsconfig.json --files -r tsconfig-paths/register ./src/main.ts",
    "start:disk": "cross-env ORM_TYPE=disk ts-node -r tsconfig-paths/register ./src/main.ts",
    "start:dev": "cross-env NODE_ENV=dev ts-node -r tsconfig-paths/register ./src/main.ts"
  },
  "author": "Paul Shannon",
  "license": "GPL-2.0",
  "devDependencies": {
    "cross-env": "^5.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-config": "^20.10.0"
  },
  "dependencies": {
    "@catsvsdogs/persistence": "1.0.0",
    "@nestjs/cli": "^5.5.0",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/typeorm": "^7.1.0",
    "@types/express": "^4.17.6",
    "@types/node": "^10.12.2",
    "@types/serverless": "^1.72.3",
    "class-transformer": "0.2.3",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "is-port-reachable": "^3.0.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.12",
    "serverless": "^1.73.1",
    "serverless-http": "^2.5.0",
    "shx": "^0.3.2",
    "sql.js": "^1.3.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.5",
    "webpack-config-dump-plugin": "^2.0.3"
  }
}
